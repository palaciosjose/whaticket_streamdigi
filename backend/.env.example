NODE_ENV=production
BACKEND_URL=YOUR_BACKEND_URL
FRONTEND_URL=YOUR_FRONTEND_URL
PROXY_PORT=8081
PORT=8081

USE_HTTPS=false
SSL_CRT_FILE=PATH_TO_SSL_CRT_FILE
SSL_KEY_FILE=PATH_TO_SSL_KEY_FILE

# CREDENCIAIS BD
DB_HOST=YOUR_DB_HOST
DB_DIALECT=postgres
DB_PORT=5432
DB_USER=YOUR_DB_USER
DB_PASS=YOUR_DB_PASS
DB_NAME=YOUR_DB_NAME

MASTER_KEY=YOUR_MASTER_KEY

TIMEOUT_TO_IMPORT_MESSAGE=1000

# Time window margin in seconds for scheduled messages (recommended 120-300s)
SCHEDULE_MARGIN_SECONDS=300

# Cron expression for schedule monitor (default every 30s).
# Values below 10 seconds may impact performance.
SCHEDULE_MONITOR_INTERVAL=*/30 * * * * *

# CREDENCIAIS DO REDIS
REDIS_URI=redis://:YOUR_REDIS_PASS@YOUR_REDIS_HOST:6379
REDIS_OPT_LIMITER_MAX=1
REDIS_OPT_LIMITER_DURATION=3000

# CREDENCIAIS BULL DO REDIS N√ÉO PRECISA USAR
# REDIS_URI_ACK=redis://:YOUR_REDIS_PASS@YOUR_REDIS_HOST:6379
# BULL_BOARD=true
# BULL_USER=YOUR_BULL_USER
# BULL_PASS=YOUR_BULL_PASS

# USER_LIMIT=10000
# CONNECTIONS_LIMIT=100000
# CLOSED_SEND_BY_ME=true

# SECRETS
JWT_SECRET=YOUR_JWT_SECRET
JWT_REFRESH_SECRET=YOUR_JWT_REFRESH_SECRET

COMPANY_TOKEN=YOUR_COMPANY_TOKEN
PERFEX_URL=YOUR_PERFEX_URL
PERFEX_MODULE=YOUR_PERFEX_MODULE

VERIFY_TOKEN=YOUR_VERIFY_TOKEN

#METODOS DE PAGAMENTO
STRIPE_PRIVATE=YOUR_STRIPE_PRIVATE_KEY
STRIPE_OK_URL=BACKEND_URL/subscription/stripewebhook
STRIPE_CANCEL_URL=FRONTEND_URL/financeiro
MP_ACCESS_TOKEN=YOUR_MP_ACCESS_TOKEN
MP_NOTIFICATION_URL=BACKEND_URL/subscription/mercadopagowebhook
ASAAS_TOKEN=YOUR_ASAAS_TOKEN
GERENCIANET_SANDBOX=false
GERENCIANET_CLIENT_ID=YOUR_GERENCIANET_CLIENT_ID
GERENCIANET_CLIENT_SECRET=YOUR_GERENCIANET_CLIENT_SECRET
GERENCIANET_PIX_CERT=YOUR_GERENCIANET_PIX_CERT
GERENCIANET_PIX_KEY=YOUR_GERENCIANET_PIX_KEY

FACEBOOK_APP_ID=YOUR_FACEBOOK_APP_ID
FACEBOOK_APP_SECRET=YOUR_FACEBOOK_APP_SECRET

# EMAIL
MAIL_HOST="YOUR_SMTP_HOST"
MAIL_USER="YOUR_EMAIL"
MAIL_PASS="YOUR_EMAIL_PASSWORD"
MAIL_FROM="YOUR_EMAIL"
MAIL_PORT="465"
